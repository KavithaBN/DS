package eclipsepackage;

import java.util.*;

public class Inorderinsertiontree
{
	class tree{
		class Node{
			int key;
			Node left,right;
			public Node (int d)
			{
				key =d;
				left = right =null;
			}
		}
	
	static Node root;
	Inorderinsertiontree()
	{
		root =null;
	
	}
	
	void insert (int key)
	{
		root = inserttree(root,key);
	}
	
	Node inserttree(Node root,int key)
	{
		if(root==null)
		{
			root = new Node(key);
		return root;
		}
	}
	if(key <root.key)
		root.left =inserttree(root.left,key)
		else if(key>root.key)
			root.right =inserttree(root.right,key);
	return root;
}
	Void inorder()
	{
		inorderrec(root);
	}
	void inorderrec (Node root)
	{
		if(root!=null)
		{
			inorderrec(root.left);
			System.out.println(root.key);
			inorderrec(root.right);
		}
	}
	public static void main(string[] args)
	{
		Inorderinsertiontree tree = new Inorderinsertiontree();
		tree.insert(50);
		tree.insert(90);
		tree.insert(10);
		tree.insert(80);
		tree.insert(20);
		tree.insert(100);
		tree.inorder();
		
	}
}